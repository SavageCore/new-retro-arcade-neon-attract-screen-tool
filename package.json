{
  "name": "new-retro-arcade-neon-attract-screen-tool",
  "version": "1.1.2",
  "description": "A tool to render New Retro Arcade: Neon attract screen video files using FFmpeg",
  "main": "./app/main.js",
  "scripts": {
    "start": "electron .",
    "test": "xo && stylelint \"app/*.css\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SavageCore/new-retro-arcade-neon-attract-screen-tool.git"
  },
  "keywords": [
    "New Retro Arcade: Neon",
    "attract",
    "screen",
    "render"
  ],
  "author": "SavageCore",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SavageCore/new-retro-arcade-neon-attract-screen-tool/issues"
  },
  "homepage": "https://github.com/SavageCore/new-retro-arcade-neon-attract-screen-tool#readme",
  "devDependencies": {
    "electron": "^9.0.3",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "xo": "^0.24.0"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.0.19",
    "@ffprobe-installer/ffprobe": "^1.0.12",
    "config-ini-parser": "^1.2.2",
    "electron-download": "^4.1.1",
    "electron-json-storage": "^4.1.6",
    "electron-unhandled": "^2.2.0",
    "eol": "^0.9.1",
    "fs-extra": "^8.1.0",
    "jquery": "^3.4.1",
    "jsonfile": "^5.0.0",
    "klaw": "^3.0.0",
    "log-writer": "0.0.9",
    "make-dir": "^3.0.0",
    "moment": "^2.24.0",
    "object-keys": "^1.1.1",
    "pkginfo": "^0.4.1",
    "pretty-bytes": "^5.2.0",
    "pretty-filename": "^0.1.0",
    "progress-stream": "^2.0.0",
    "request": "^2.88.0",
    "semver": "^6.2.0",
    "sortablejs": "^1.9.0",
    "speedometer": "^1.1.0",
    "sumchecker": "^3.0.0",
    "throttleit": "^1.0.0",
    "winreg": "^1.2.4",
    "xml-objects": "^1.0.1",
    "xml-writer": "^1.7.0",
    "xmlfmt": "^1.0.0",
    "yauzl": "^2.10.0"
  },
  "xo": {
    "rules": {
      "max-nested-callbacks": [
        1,
        10
      ],
      "block-scoped-var": 0,
      "no-async-promise-executor": 0
    },
    "prettifier": true
  }
}
